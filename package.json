    "@nestjs/platform-express": "8.0.0",
    "@nestjs/swagger": "5.0.9",
    "@nestjs/typeorm": "8.0.2",
    "await-to-js": "3.0.0",
    "body-parser": "^1.20.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.3",
    "helmet": "4.6.0",
    "joi": "17.4.2",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "mssql": "7.3.0",
    "mustache": "4.2.0",
